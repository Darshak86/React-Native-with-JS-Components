{
  "name": "RNC_JS_CLI_Demo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "pandroid": "yarn bundle && cd android && gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "clean": "cd android && gradlew clean"
  },
  "dependencies": {
    "@react-native-community/checkbox": "^0.5.12",
    "@react-native-community/datetimepicker": "^6.3.2",
    "@react-native-community/slider": "^4.3.1",
    "@react-native-picker/picker": "^2.4.4",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "link": "^1.5.1",
    "react": "18.0.0",
    "react-native": "^0.69.3",
    "react-native-fast-image": "^8.5.11",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
